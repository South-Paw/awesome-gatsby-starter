  "license": "MIT",
  "author": {
    "name": "Alex Gabites",
    "email": "hello@southpaw.co.nz",
    "url": "http://southpaw.co.nz/"
  },
  "files": [],
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/South-Paw/awesome-gatsby-starter"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
